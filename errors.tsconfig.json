{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": false,
    "noEmit": false,
    "skipLibCheck": true,
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "baseUrl": "./errors"
  },
  "include": ["errors/**/*.ts"],
  "references": []
}
